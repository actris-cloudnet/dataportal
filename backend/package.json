{
   "name": "dataportal",
   "version": "2.11.1",
   "description": "Actris cloud remote sensing data portal",
   "main": "build/server.js",
   "directories": {
      "doc": "docs"
   },
   "dependencies": {
      "archiver": "^4.0.2",
      "aws-sdk": "^2.848.0",
      "axios": "^0.21.1",
      "express": "^4.17.1",
      "mock-aws-s3": "^4.0.1",
      "nodemon": "^2.0.7",
      "pg": "^8.0.3",
      "pg-query-stream": "^4.0.0",
      "reflect-metadata": "^0.1.10",
      "sass": "^1.32.8",
      "typeorm": "^0.2.31",
      "uuid": "^8.3.2",
      "validator": "^12.2.0",
      "xml2js": "^0.4.23"
   },
   "devDependencies": {
      "@types/adm-zip": "^0.4.33",
      "@types/archiver": "^3.1.1",
      "@types/express": "^4.17.11",
      "@types/express-xml-bodyparser": "^0.3.2",
      "@types/graceful-fs": "^4.1.5",
      "@types/jest": "^25.2.3",
      "@types/mock-aws-s3": "^2.6.1",
      "@types/node": "^13.13.45",
      "@types/request-promise-native": "^1.0.17",
      "@types/selenium-webdriver": "^4.0.11",
      "@types/uuid": "^8.3.0",
      "@types/validator": "^12.0.1",
      "@types/xml2js": "^0.4.8",
      "@typescript-eslint/eslint-plugin": "^2.34.0",
      "@typescript-eslint/parser": "^2.34.0",
      "adm-zip": "^0.4.16",
      "eslint": "^6.8.0",
      "jest": "^26.6.3",
      "request": "^2.88.2",
      "request-promise-native": "^1.0.9",
      "selenium-webdriver": "^4.0.0-beta.1",
      "ts-jest": "^26.5.1",
      "ts-node": "^9.1.1",
      "typescript": "^4.1.5"
   },
   "scripts": {
      "reset-db": "ts-node -T src/fixtures.ts fixtures TRUNCATE",
      "test": "npm run reset-db && jest tests/integration/parallel && jest --runInBand tests/integration/sequential",
      "e2e-test": "set -e; for f in tests/e2e/*; do npm run reset-db && jest $f; done",
      "selenium-test": "npm run reset-db && jest --runInBand tests/selenium",
      "start": "node build/server.js",
      "start-dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'npx ts-node -T' src/server.ts",
      "start-test": "node tests/lib/service-mocks.js & npm run start-dev",
      "stop": "echo 'Use ./control script at project root'",
      "build": "tsc",
      "lint": "eslint . --ext .js,.ts",
      "lint-fix": "eslint . --ext .js,.ts --fix"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/actris-cloudnet/dataportal.git"
   },
   "author": "",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/actris-cloudnet/dataportal/issues"
   },
   "homepage": "https://github.com/actris-cloudnet/dataportal#readme"
}
