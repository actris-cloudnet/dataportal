    "uplot": "^1.6.30",
    "v-calendar": "^3.1.0",
    "vue": "^3.5.0",
    "vue-matomo": "^4.2.0",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.1",
    "@types/escape-html": "^1.0.2",
    "@types/jsdom": "^21.1.2",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet-draw": "^1.0.6",
    "@types/node": "^18.17.14",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/eslint-plugin": "^1.1.31",
