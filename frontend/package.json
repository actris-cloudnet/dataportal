    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.0",
    "start-server-and-test": "^2.0.0",
    "the-new-css-reset": "^1.9.0",
    "typescript": "^5.5.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.0"
  }
